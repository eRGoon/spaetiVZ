<% title "Product" %>

<table>
  <tr>
    <th>Sid</th>
    <th>Name</th>
  </tr>
  <% for products in @products %>
    <tr>
      <td><%= products.sID %></td>
      <td><%= products.name %></td>
      <td><%= link_to "Show", products %></td>
      <td><%= link_to "Edit", edit_product_path(products) %></td>
      <td><%= link_to "Destroy", products, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Product", new_product_path %></p>
